{
	"name": "MyTheme",
	"tokenColors": [
		{
			"settings": {
				"foreground": "#93a1a1bb"
			}
		},
		{
			"scope": [
				"meta.embedded",
				"source.groovy.embedded"
			],
			"settings": {
				"foreground": "#657B83"
			}
		},
		{
			"name": "Comment",
			"scope": [
				"comment",
				"comment.block",
				"comment.block.documentation",
				"comment.line"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#93a1a1bb"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.regexp",
				"string.interpolated",
				"string.quoted",
				"string.quoted.single",
				"string.quoted.double",
				"string.quoted.triple",
				"string.quoted.other",
				"string.unquoted",
				"string.other"
			],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"name": "Variable",
			"scope": [
				"variable.name",
				"variable.other.readwrite",
				"variable.parameter",
				"variable.language",
				"variable.other"
			],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"scope": [
				"constant",
				"constant.character",
				"constant.character.escape",
				"constant.numeric",
				"constant.numeric.integer",
				"constant.numeric.float",
				"constant.numeric.hex",
				"constant.numeric.octal",
				"constant.regexp",
				"constant.rgb-value",
				"constant.other"
			],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"name": "Built-in constant",
			"scope": [
				"constant.language",
				"meta.preprocessor"
			],
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword",
				"keyword.control",
				"keyword.other"
			],
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"keyword.operator",
				"keyword.operator.logical",
				"keyword.operator.assignment",
				"keyword.operator.new"
			],
			"settings": {
				"foreground": "#268bd2",
				"fontStyle": "bold"
			}
		},
		{
			"scope": "keyword.operator.assignment",
			"settings": {
				"foreground": "#dc322f",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage",
				"storage.modifier",
				"storage.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#657b83"
			}
		},

		{
			"name": "Library Syntax Hilight",
			"scope": [
				"support",
				"support.class",
				"support.constant",
				"support.function",
				"support.other",
				"support.other.variable",
				"support.type",
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"scope": "support.variable",
			"settings": {
				"foreground": "#d33682"
			}
		},
		{
			"name": "Library function",
			"scope": "support.function",
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Support.construct",
			"scope": [
				"support.function.construct",
				"keyword.other.new"
			],
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "Library class/type",
			"scope": [
				"support.type",
				"support.class"
			],
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "Library Exception",
			"scope": "support.type.exception",
			"settings": {
				"foreground": "#CB4B16"
			}
		},
		{
			"scope": [
				"punctuation.definition.string.begin",
				"punctuation.definition.string.end",
				"punctuation.separator",
				"punctuation.separator.continuation",
				"punctuation.terminator"
			],
			"settings": {
				"foreground": "#93a1a1"
			}
		},
		{
			"name": "Variable start",
			"scope": "punctuation.definition.variable",
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "Embedded code markers",
			"scope": [
				"punctuation.section.embedded.begin",
				"punctuation.section.embedded.end"
			],
			"settings": {
				"foreground": "#D30102"
			}
		},
		{
			"name": "Tag start/end",
			"scope": [
				"punctuation.definition.tag.begin",
				"punctuation.definition.tag.end"
			],
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"scope": "strong",
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"scope": [
				"entity",
				"entity.name",
				"entity.name.type",
				"entity.name.method",
				"entity.name.section",
				"entity.name.selector",
				"entity.other",
				"entity.other.attribute-name",
				],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"name": "Class name",
			"scope": [
				"entity.name.class",
				"entity.name.type"
			],
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Function name",
			"scope": "entity.name.function",
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Inherited class",
			"scope": "entity.other.inherited-class",
			"settings": {}
		},
		{
			"name": "Tag name",
			"scope": "entity.name.tag",
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Tag attribute",
			"scope": "entity.other.attribute-name",
			"settings": {
				"foreground": "#93A1A1"
			}
		},
		{
			"scope": [
				"meta",
				"meta.block",
				"meta.cast",
				"meta.class",
				"meta.function",
				"meta.function-call",
				"meta.return-type",
				"meta.selector",
				"meta.tag",
				"meta.type",
				"meta.type.annotation"	
			],
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"scope": "meta.preprocessor",
			"settings": {
				"foreground": "#268bd2"
			}
		},
		{
			"name": "diff: header",
			"scope": [
				"meta.diff",
				"meta.diff.header"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#E0EDDD"
			}
		},
		{
			"scope": [
				"markup",
				"markup.bold",
				"markup.changed",
				"markup.deleted",
				"markup.heading",
				"markup.inline.raw",
				"markup.inserted",
				"markup.italic",
				"markup.list",
				"markup.list.numbered",
				"markup.list.unnumbered",
				"markup.other",
				"markup.quote",
				"markup.raw",
				"markup.underline",
				"markup.underline.link"
			],	
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"name": "diff: deleted",
			"scope": "markup.deleted",
			"settings": {
				"fontStyle": "",
				"foreground": "#dc322f"
			}
		},
		{
			"name": "diff: changed",
			"scope": "markup.changed",
			"settings": {
				"fontStyle": "",
				"foreground": "#cb4b16"
			}
		},
		{
			"name": "diff: inserted",
			"scope": "markup.inserted",
			"settings": {
				"foreground": "#219186"
			}
		},
		{
			"name": "Markup Quote",
			"scope": "markup.quote",
			"settings": {
				"foreground": "#859900"
			}
		},
		{
			"name": "Markup Lists",
			"scope": "markup.list",
			"settings": {
				"foreground": "#B58900"
			}
		},
		{
			"name": "Markup Styling",
			"scope": [
				"markup.bold",
				"markup.italic"
			],
			"settings": {
				"foreground": "#D33682"
			}
		},
		{
			"name": "Markup Inline",
			"scope": "markup.inline.raw",
			"settings": {
				"fontStyle": "",
				"foreground": "#2AA198"
			}
		},
		{
			"name": "Markup Headings",
			"scope": "markup.heading",
			"settings": {
				"foreground": "#268BD2"
			}
		},
		{
			"name": "Markup Setext Header",
			"scope": "markup.heading.setext",
			"settings": {
				"fontStyle": "",
				"foreground": "#268BD2"
			}
		},
		{
			"scope": "emphasis",
			"settings": {
				"foreground": "#657b83"
			}
		},
		{
			"scope": [
				"invalid",
				"invalid.deprecated",
				"invalid.illegal"
				],
			"settings": {
				"foreground": "#657b83"
			}
		}
	],
